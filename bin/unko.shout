#!/bin/bash

echo-sd ${1+"$@"}
c="${C:-👑}"	# Crown
u="${U:-💩}"	# Unko
e="${E:-👁}"	# Eyes
n="${N:-👃}"	# Nose
m="${M:-👄}"	# Mouth
l="${L:-（}"	# Left side
r="${R:-）}"	# Right side
s="${S:-　}"	# Space
cat <<EOF
$s$s$s$s$s$s$c
$s$s$s$s$l$u$u$u$r
$s$s$s$l$u$e$u$e$u$r
$s$s$l$u$u$u$n$u$u$u$r
$s$l$u$u$u$u$m$u$u$u$u$r
EOF
